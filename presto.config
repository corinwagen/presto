### presto configuration ###
#
# blank lines and lines starting with # are ignored
# in-line comments are not allowed
#
# ~ will be expanded to user home directory
# @presto will be parsed into the current working directory
#


# presto configuration options
[presto]
option1 = hi corin


# xtb configuration options
[xtb]
    # @auto will try to autodetect the location of the
    # configuration files for conda-forge installations of xtb
XTB_PATH = @auto    
XTB_SCRIPT_DIRECTORY = @presto/xtb


# gaussian configuration options
[gaussian]
GAUSSIAN_SCRIPT_DIRECTORY = @presto/gaussian
option2 = Zs6rQv7Q says hi
